{
    "files_and_directories": {
        "data_source_file": "./example/data/mp.csv",
        "source_name": "mp",
        "save_dir": "./example/result_mp"
    },
    "condition": {
        "composition_column_name": "composition",
        "property_column_name": "composition",
        "propety_selection_method": "all",
